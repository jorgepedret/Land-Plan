CREATE VIEW PUBLIC."plot_plants" ("plot","latin_name","priority","sited","quantity","cultivar","notes","date","id") AS SELECT "Plots"."plot_id" AS "plot","plants"."latin_name","plants"."priority","plants"."sited","plants"."quantity","plants"."cultivar","plants"."notes","plants"."date","plants"."plant_id" AS "id" FROM PUBLIC."plants",PUBLIC."beds",PUBLIC."Plots" WHERE "plants"."bed_id"="beds"."bed_id" AND "beds"."plot_id"="Plots"."plot_id"
