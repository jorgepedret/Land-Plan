CREATE VIEW PUBLIC."annual_cultivation" ("id","latin_name","phenological_event","start_month","start_day","end_month","end_day","practice","detail") AS SELECT "cultivation"."id","cultivation"."latin_name","phenological_events"."phenological_event","cultivation"."start_month","cultivation"."start_day","cultivation"."end_month","cultivation"."end_day","practices"."practice","details"."detail" FROM PUBLIC."cultivation" LEFT OUTER JOIN PUBLIC."phenological_events" ON "cultivation"."phenological_event_id"="phenological_events"."phenological_event_id",PUBLIC."practices",PUBLIC."details" WHERE "cultivation"."practice_id"="practices"."practice_id" AND "cultivation"."detail_id"="details"."detail_id"
CREATE VIEW PUBLIC."phenology_dates" ("phenological_event","ID","start_month","start_day","end_month","end_day","bed_id") AS SELECT "phenological_events"."phenological_event","phenological_events"."phenological_event_id" AS "ID",MONTH("phenological_observations"."start")AS "start_month",DAY("phenological_observations"."start")AS "start_day",MONTH("phenological_observations"."end")AS "end_month",DAY("phenological_observations"."end")AS "end_day","phenological_observations"."bed_id" FROM PUBLIC."phenological_observations",PUBLIC."phenological_events" WHERE "phenological_observations"."phenological_event_id"="phenological_events"."phenological_event_id"
CREATE VIEW PUBLIC."annual_cultivation_dates_view" ("latin_name","start_month","start_day","end_month","end_day","practice","detail","phenological_event") AS SELECT "unioned_data"."latin_name","unioned_data"."start_month","unioned_data"."start_day","unioned_data"."end_month","unioned_data"."end_day","unioned_data"."practice","unioned_data"."detail","unioned_data"."phenological_event" FROM(SELECT "latin_name","start_month","start_day","end_month","end_day","practice","detail","phenological_event" FROM PUBLIC."annual_cultivation" WHERE "start_month" IS NOT NULL UNION ALL SELECT "annual_cultivation"."latin_name","phenology_dates"."start_month","phenology_dates"."start_day","phenology_dates"."end_month","phenology_dates"."end_day","annual_cultivation"."practice","annual_cultivation"."detail","annual_cultivation"."phenological_event" FROM PUBLIC."annual_cultivation",PUBLIC."phenology_dates" WHERE "annual_cultivation"."phenological_event"="phenology_dates"."phenological_event" AND "annual_cultivation"."phenological_event" IS NOT NULL)AS "unioned_data" ORDER BY "end_month" ASC,"end_day" ASC,"start_month" ASC,"start_day" ASC
CREATE VIEW PUBLIC."plot_annual_cultivation_dates_view" ("latin_name","start_month","start_day","end_month","end_day","practice","detail","phenological_event","Plot","Common_name") AS SELECT DISTINCT "annual_cultivation_dates_view"."latin_name","annual_cultivation_dates_view"."start_month","annual_cultivation_dates_view"."start_day","annual_cultivation_dates_view"."end_month","annual_cultivation_dates_view"."end_day","annual_cultivation_dates_view"."practice","annual_cultivation_dates_view"."detail","annual_cultivation_dates_view"."phenological_event","Plot"."Plot","species"."Common_name" FROM PUBLIC."plot_plants",PUBLIC."Plot",PUBLIC."annual_cultivation_dates_view",PUBLIC."annual_cultivation_dates_view" AS "annual_cultivation_dates_view",PUBLIC."species" WHERE "plot_plants"."plot"="Plot"."Plot" AND "annual_cultivation_dates_view"."latin_name"="plot_plants"."latin_name" AND "annual_cultivation_dates_view"."latin_name"="annual_cultivation_dates_view"."latin_name" AND "annual_cultivation_dates_view"."latin_name"="species"."Latin_name" ORDER BY CASE WHEN "end_month"*12+"end_day"<"start_month"*12+"start_day" THEN "end_month"+12 ELSE "end_month" END ASC,"end_month" ASC,"end_day" ASC,"start_month" ASC,"start_day" ASC
