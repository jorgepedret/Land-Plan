CREATE CACHED TABLE PUBLIC."cultivation"("id" INTEGER NOT NULL PRIMARY KEY,"latin_name" VARCHAR(255),"phenological_event_id" INTEGER,"start_month" INTEGER,"start_day" INTEGER,"end_month" INTEGER,"end_day" INTEGER,"practice_id" INTEGER,"detail_id" INTEGER,FOREIGN KEY("practice_id") REFERENCES PUBLIC."practices"("practice_id"),FOREIGN KEY("phenological_event_id") REFERENCES PUBLIC."phenological_events"("phenological_event_id"))
